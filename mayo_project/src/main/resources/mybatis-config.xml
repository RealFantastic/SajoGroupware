<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
<settings>
	<setting name="jdbcTypeForNull" value="NULL"/>
</settings>
<typeAliases>
	<typeAlias type="com.group.mayo.employee.domain.Employee" alias="Employee"/>
	<typeAlias type="com.group.mayo.project.domain.Project" alias="Project"/>
	<typeAlias type="com.group.mayo.work.domain.Work" alias="Work"/>
	<typeAlias type="com.group.mayo.company.domain.Company" alias="Company"/>
	<typeAlias type="com.group.mayo.department.domain.Dept" alias="Dept"/>
</typeAliases>
<mappers>
	<mapper resource="mappers/employee-mapper.xml"/>
	<mapper resource="mappers/project-mapper.xml"/>
	<mapper resource="mappers/work-mapper.xml"/>
	<mapper resource="mappers/company-mapper.xml"/>
</mappers>
</configuration>
